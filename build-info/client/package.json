    "@angular/platform-browser": "19.2.8",
    "@angular/platform-browser-dynamic": "19.2.8",
    "@angular/router": "19.2.8",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular/build": "19.2.8",
    "@angular/cli": "19.2.9",
    "@angular/compiler-cli": "19.2.8",
    "local-web-server": "5.4.0",
    "replace-in-file": "7.2.0",
