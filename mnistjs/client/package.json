    "@angular/router": "20.2.3",
    "@ionic/angular": "8.7.3",
    "@tensorflow/tfjs": "4.22.0",
    "brain.js": "1.6.1",
    "ionicons": "8.0.13",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "20.2.1",
