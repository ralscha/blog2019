{
  "name": "googlefont",
  "version": "0.0.1",
  "scripts": {
    "build": "ng build",
    "dl-font-fresca": "goog-webfont-dl -o src/font-fresca.css -p assets/Fresca -d src/assets/Fresca -a Fresca",
    "dl-font-opensans": "goog-webfont-dl -o src/font-opensans.css -p assets/OpenSans -d src/assets/OpenSans -a \"Open Sans\"",
    "ng": "ng",
    "postinstall": "npm run dl-font-opensans && npm run dl-font-fresca",
    "serve-dist": "ws --hostname localhost -d dist/app/browser -p 1234 -o --log.format stats",
    "start": "ng serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.1.3",
    "@angular/common": "20.1.3",
    "@angular/compiler": "20.1.3",
    "@angular/core": "20.1.3",
    "@angular/forms": "20.1.3",
    "@angular/platform-browser": "20.1.3",
    "@angular/router": "20.1.3",
    "@ionic/angular": "8.6.5",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "20.1.3",
    "@angular/cli": "20.1.3",
    "@angular/compiler-cli": "20.1.3",
    "@ionic/angular-toolkit": "12.2.0",
    "goog-webfont-dl": "1.0.4",
    "local-web-server": "5.4.0",
    "typescript": "5.8.3"
  }
}
